<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence>
				<xs:element minOccurs="0" maxOccurs="unbounded" ref="item"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="item">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="for"/>
			</xs:sequence>
			<xs:attribute name="id" use="required" type="xs:integer"/>
			<xs:attribute name="name" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="for">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" ref="add"/>
				<xs:element minOccurs="0" ref="enchant"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="add">
		<xs:complexType>
			<xs:sequence>
				<xs:element minOccurs="0" ref="and"/>
			</xs:sequence>
			<xs:attribute name="order" use="required" type="xs:NMTOKEN"/>
			<xs:attribute name="stat" use="required" type="xs:NCName"/>
			<xs:attribute name="val" use="required" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="and">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="using"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="using">
		<xs:complexType>
			<xs:attribute name="slotitem" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="enchant">
		<xs:complexType>
			<xs:attribute name="order" use="required" type="xs:NMTOKEN"/>
			<xs:attribute name="stat" use="required" type="xs:NCName"/>
			<xs:attribute name="val" use="required" type="xs:integer"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
