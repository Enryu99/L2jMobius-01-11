<?xml version="1.0" encoding="UTF-8"?>
<list xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../xsd/items.xsd">
	<item id="49782" name="Balthus Knights Supply" type="EtcItem">
		<!-- Double-click to receive 3 Balthus Knight Marks. (Can be used only once a day). Cannot be exchanged, dropped or sold. Can be stored in a private warehouse. This item will be deleted after the event is over. -->
		<set name="icon" val="icon.etc_ev_kalie_treasure_box" />
		<set name="default_action" val="SKILL_REDUCE" />
		<set name="reuse_delay" val="86400000" /> <!-- 1 Day -->
		<set name="material" val="PAPER" />
		<set name="is_tradable" val="false" />
		<set name="is_dropable" val="false" />
		<set name="is_sellable" val="false" />
		<set name="handler" val="ItemSkills" />
		<set name="is_destroyable" val="false" />
		<skills>
			<skill id="39170" level="1" /> <!-- Balthus Knights Supply -->
		</skills>
	</item>
	<item id="49783" name="Sibi Coin" type="EtcItem">
		<!-- A coin paid for activity while disguised as a Balthus Knight. Event Manager Sibi will be able to exchange them for Sibi's Suspicious Supply Items. This item will be deleted after the event is over. -->
		<set name="icon" val="icon.ev_balthus_coin" />
		<set name="is_sellable" val="false" />
		<set name="is_stackable" val="true" />
		<set name="material" val="PAPER" />
	</item>
	<item id="49784" name="Balthus Knight Mark" type="EtcItem">
		<!-- Acquired XP/SP +10% for 1 hour. Intended for Lv. 20 and higher. Cannot be exchanged, dropped, or sold in a private store. Can be stored in a private warehouse and transferred between characters on the same account. -->
		<set name="icon" val="icon.ev_balthus_token" />
		<set name="default_action" val="SKILL_REDUCE" />
		<set name="reuse_delay" val="60000" /> <!-- 1 minute -->
		<set name="material" val="PAPER" />
		<set name="is_tradable" val="false" />
		<set name="is_dropable" val="false" />
		<set name="is_sellable" val="false" />
		<set name="is_stackable" val="true" />
		<set name="is_freightable" val="true" />
		<set name="handler" val="ItemSkills" />
		<skills>
			<skill id="39171" level="1" /> <!-- Member of Balthus Knights -->
		</skills>
	</item>
	<item id="49798" name="Cheer Stamen" additionalName="Event" type="Weapon">
		<!-- A cheering tool issued to commemorate the 12th anniversary of Lineage II. Hold on to your dreams, as the day will come when they are realized. Equip to enable the Firecracker skill. -->
		<set name="icon" val="icon.ev_12th_weapon" />
		<set name="default_action" val="EQUIP" />
		<set name="weapon_type" val="DUAL" />
		<set name="bodypart" val="lrhand" />
		<set name="immediate_effect" val="true" />
		<set name="material" val="STEEL" />
		<set name="weight" val="150" />
		<set name="soulshots" val="2" />
		<set name="spiritshots" val="2" />
		<set name="is_tradable" val="false" />
		<set name="is_dropable" val="false" />
		<set name="is_sellable" val="false" />
		<set name="is_freightable" val="true" />
		<stats>
			<stat type="pAtk">12</stat>
			<stat type="mAtk">12</stat>
			<stat type="rCrit">8</stat>
			<stat type="pAtkSpd">200</stat>
			<stat type="randomDamage">1</stat>
			<stat type="pAtkRange">40</stat>
		</stats>
	</item>
</list>
